.horizontal {
    flex-direction: row;
}
.vertical {
    flex-direction: column;
}

.hidden {
    visibility: hidden;
}

.minimize {
    position: absolute;
    height: 0;
    width: 0;
}

.grow {
    flex-grow: 1;
}

